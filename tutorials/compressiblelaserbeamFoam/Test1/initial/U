FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    top
    {
        type            noSlip;
    }
    
    bottom
    {
        type            noSlip;
    }
    
    right
    {
        type            noSlip;
    }

    left
    {
        type            noSlip;
    }
    
    front
    {
        type            noSlip;
    }
    
    back
    {
        type pressureInletOutletVelocity;
        value uniform (0 0 0);
    }   
  
}

