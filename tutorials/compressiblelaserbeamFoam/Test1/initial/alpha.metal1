dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    top
    {
    
    type            zeroGradient;
    

    }
    
    bottom
    {
       type            zeroGradient;
    }
    
    right
    {
       type            zeroGradient;
    }

    left
    {
       type            zeroGradient;
    }
    
    front
    {
       type            zeroGradient;
    }
    
    back
    {
type            zeroGradient;
    }
   
}

