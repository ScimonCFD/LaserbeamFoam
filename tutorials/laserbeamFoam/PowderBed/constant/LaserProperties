FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      PhaseFieldProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


timeVsLaserPosition
{
    file    "$FOAM_CASE/constant/timeVsLaserPosition0";
    outOfBounds clamp;
}

timeVsLaserPower
{
    file    "$FOAM_CASE/constant/timeVsLaserPower0";
    outOfBounds clamp;
}

// Laser angle (normalised in the code)
V_incident (0 1 0.25);

// Laser radius
laserRadius 0.00025;

// number of Rays per cell
N_sub_divisions	1;

// Laser physical properties
wavelength      1.064e-6;
e_num_density   5.83e29;

// Laser profile type (defaults to 2)
Radius_Flavour 3.0;


