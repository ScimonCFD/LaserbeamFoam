FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      phaseProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Interface tracking method: isoAdvector or MULES
interfaceTrackingScheme isoAdvector;

phases          (metal gas);

// phase properties for the metal and gas phases
metal
{
    transportModel      Newtonian;
    nu                  1.35e-06;
    rho                 4420;
    Tsolidus            1928.15;
    Tliquidus           1878;
    LatentHeat          2.95653e5;
    beta                5.0e-4;
    poly_kappa          (10 0.015 0 0 0 0 0 0);
    //poly_cp           (244.8 9.587e-1 -3.77e-4 6.5e-8 -4.14e-12 0 0 0);
    poly_cp             (520 0.075 0 0 0 0 0 0);
    elec_resistivity    2.0e-6; 
}

gas
{
    transportModel      Newtonian;
    nu                  1.0e-05;
    rho                 1.78;
    Tsolidus            1.0;
    Tliquidus           10.0;
    LatentHeat          1;
    beta                1.0e-4;
    poly_kappa          (0.04 0.0 0 0 0 0 0 0);
    poly_cp             (520 0.0 0 0 0 0 0 0);
}

// phase change properties
sigma                   1.7;
Marangoni_Constant	    -3.6e-4; //Surface Tension and Viscosity of Industrial Alloys from Parabolic Flight Experiments - Results of the ThermoLab Project
p0		                101325.0;
Tvap                    3558.0;
Mm                      4.786e-2;
