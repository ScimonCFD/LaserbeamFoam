FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      phaseProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
// phase properties for the metal and gas phases
interfaceTrackingScheme isoAdvector;

phases          (metal gas);
// phase proerties for the metal and gas phases

metal
{
    transportModel      Newtonian; // set
    nu                  0.8e-06;   // set
    rho                 7280;      // set
    Tsolidus            1658;      // set
    Tliquidus           1723.0;    // set
    LatentHeat          2.7e5;   //set - but there is no in laserMeltFoam
    beta                5.0e-6; //set - but there is no in laserMeltFoam
    poly_kappa          (10 0.015  0 0 0 0 0 0);  //set
    poly_cp             (412.0  2e-1    -2e-5 0 0 0 0 0);   //set
    elec_resistivity    0.1e-6;  // set
}

gas
{
    transportModel      Newtonian; //set
    nu                  1.5e-05;  //set
    rho                 1.6337;  //set
    Tsolidus            1.0;//set - but there is no in laserMeltFoam
    Tliquidus           10.0;//set - but there is no in laserMeltFoam
    LatentHeat          1; //set - but there is no in laserMeltFoam
    beta                4.0e-5; //set - but there is no in laserMeltFoam
    poly_kappa          (0.0177 0.0 0 0 0 0 0 0);  //set
    poly_cp             (520 0.0 0 0 0 0 0 0);  //set
}

// phase change properties
sigma                   1.4;  // set
Marangoni_Constant      -4e-4;  //set
p0                      101000;  //set
Tvap                    3090.0; //set
Mm                      0.05593; //set
LatentHeatVap           7.45e6;  //set
TRef                    TRef [0 0 0 1 0 0 0]   298.0;
emS                     0.4;
emL                     0.1;
hC                      hC   [1 0 -3 -1 0 0 0]    40;

